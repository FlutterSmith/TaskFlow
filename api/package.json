    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.8.0",
    "redis": "^4.7.0",
    "winston": "^3.15.0",
    "multer": "^2.0.2",
    "sharp": "^0.34.4",
    "zod": "^3.23.8",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
